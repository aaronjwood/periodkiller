<Window x:Class="PeriodKiller.Windows.Collisions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Collisions" Height="300" Width="300">
    <DockPanel>
        <ListView x:Name="duplicateItems">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Type" DisplayMemberBinding="{Binding Type}"/>
                    <GridViewColumn Header="Original" DisplayMemberBinding="{Binding Original}"/>
                    <GridViewColumn Header="Renamed" DisplayMemberBinding="{Binding Renamed}"/>
                    <GridViewColumn Header="Collides With" DisplayMemberBinding="{Binding Collides}"/>
                </GridView>
            </ListView.View>
        </ListView>

    </DockPanel>
</Window>
